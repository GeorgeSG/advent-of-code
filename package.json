    "node-fetch": "2",
    "ramda": "^0.28.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.1"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "@types/ramda": "^0.28.20",
    "typescript": "^4.9.3"
